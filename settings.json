{
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-20240620"
    },
    "version": "2"
  },
  "base_keymap": "VSCode",
  "theme": "VSCode Dark Modern",
  "ui_font_family": "JetBrainsMono Nerd Font",
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "vim_mode": true,
  "ui_font_size": 18,
  "buffer_font_size": 18,
  "relative_line_numbers": true,
  "autosave": "off",
  "format_on_save": "off",
  "soft_wrap": "editor_width",
  "current_line_highlight": "gutter",
  "cursor_blink": false,
  "code_actions_on_format": {
    "source.organizeImports": true
  },
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "experimental.theme_overrides": {
    "background.appearance": "blurred",
    "background": "#3a352fC0",
    "editor.gutter.background": "#000000C0",
    "tab_bar.background": "#000000C0",
    "editor.background": "#000000C0",
    "terminal.background": "#000000C0",
    "toolbar.background": "#000000C0",
    "tab.active_background": "#101010C0",
    "tab.inactive_background": "#000000A0",
    "status_bar.background": "#000000C0",
    "title_bar.background": "#000000C0",
    "panel.background": "#000000C0",
    "border.variant": "#000000A0",
    "scrollbar.track.border": "#000000A0",
    "editor.active_line.background": "#000000A0"
  }
}
